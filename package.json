{
  "name": "website",
  "version": "2.0.0",
  "description": "My personal website",
  "repository": "https://github.com/bleonard252/blakes.dev",
  "author": "Blake Leonard <me@blakes.dev>",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle:icons": "bundle-files src/_includes/icons/*",
    "build:themes": "theme-generator",
    "build:pcss": "postcss src/_includes/styles/main.pcss -o dist/styles/main.css",
    "build:11ty": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js",
    "build:tsc": "tsc --outDir dist/ --target es2019 --moduleResolution node --jsx react --jsxFactory h --jsxFragmentFactory Fragment",
    "build": "npm-run-all -s bundle:icons build:**",
    "watch:tsc": "tsc --outDir dist/ --target es2019 --moduleResolution node --jsx react --jsxFactory h --jsxFragmentFactory Fragment --watch",
    "watch:pcss": "postcss src/_includes/styles/main.pcss -o dist/styles/main.css --watch",
    "watch:11ty": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --serve",
    "watch": "npm-run-all -s bundle:icons build:themes -p watch:**",
    "serve": "npm-run-all watch"
  },
  "license": "PWL",
  "private": true,
  "engines": {
    "pnpm": "^7.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@iconify/react": "^3.2.2",
    "@private/bundle-files": "link:./tools/bundle-files",
    "@private/theme-generator": "link:./tools/generate-themes",
    "@types/node": "^18.14.1",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.15",
    "esbuild-register": "^3.4.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "preact-render-to-string": "^5.2.6",
    "tailwindcss": "^3.2.7",
    "typescript-require": "^0.3.0"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-webc": "^0.11.0",
    "@11ty/is-land": "^3.0.2",
    "@11ty/webc": "^0.11.1",
    "@jamshop/eleventy-plugin-mdx": "^0.0.17",
    "@tailwindcss/typography": "^0.5.9",
    "htm": "^3.1.1",
    "iconify-icon": "^1.0.7",
    "nunjucks": "^3.2.3",
    "preact": "^10.13.0",
    "react": "npm:@preact/compat@^17.1.2",
    "react-dom": "npm:@preact/compat@^17.1.2",
    "sanitize-html": "^2.10.0",
    "typescript": "^4.9.5"
  }
}
