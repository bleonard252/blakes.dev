{
  "name": "website",
  "version": "2.0.0",
  "description": "My personal website",
  "repository": "https://github.com/bleonard252/blakes.dev",
  "homepage": "https://blakes.dev",
  "author": "Blake Leonard <me@blakes.dev>",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:themes": "theme-generator",
    "build:pcss": "postcss src/_includes/styles/main.pcss -o dist/styles/main.css",
    "build:11ty": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js",
    "build:tsc": "tsc",
    "build:makestatic": "mkdir static/bridges -p && cp src/bridges/bridges.11tydata.json static/bridges/data.json",
    "build": "npm-run-all -s build:pcss build:themes build:tsc build:makestatic build:11ty",
    "watch:tsc": "tsc --watch",
    "watch:pcss": "postcss src/_includes/styles/main.pcss -o dist/styles/main.css --watch",
    "watch:11ty": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --serve",
    "watch": "npm-run-all -p watch:tsc watch:pcss watch:11ty",
    "serve": "npm-run-all watch"
  },
  "license": "PWL",
  "private": true,
  "engines": {
    "pnpm": "^7.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "@11ty/eleventy-plugin-webc": "^0.11.0",
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@jamshop/eleventy-plugin-mdx": "^0.0.17",
    "@private/theme-generator": "link:tools/generate-themes",
    "@tailwindcss/typography": "^0.5.9",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.14.1",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.15",
    "eleventy-plugin-babel": "^1.0.1",
    "esbuild-register": "^3.4.2",
    "megalodon": "^5.4.2",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.4",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "preact-render-to-string": "^5.2.6",
    "tailwindcss": "^3.4.1",
    "eleventy-plugin-toc": "^1.1.5",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "typescript-require": "^0.3.0"
  },
